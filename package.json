{
  "name": "quark",
  "version": "0.0.1",
  "description": "A new way to control electronics",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "ts-node copyicons.ts && tsc && electron .",
    "link": "npm install ./../@squirtle/api",
    "native": "electron-builder install-app-deps",
    "compile": "tsc",
    "test": "mocha",
    "pack-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds",
    "dist-win": "electron-builder --x64 --win --prepackaged release-builds/Quark-win32-x64",
    "build-linux": "electron-builder --x64 --linux",
    "build-mac": "electron-builder --x64 --mac",
    "build-win": "npm run pack-win && npm run dist-win",
    "build": "npm install && ts-node copyicons.ts && tsc && electron-builder ."
  },
  "dependencies": {
    "@capacitor/electron": "^1.0.0-beta.0",
    "@squirtle/api": "file:../@squirtle/api",
    "chokidar": "^2.0.4",
    "electron-is-dev": "^0.3.0",
    "electron-store": "^2.0.0",
    "find": "^0.2.9",
    "find-process": "^1.2.1",
    "fs-extra": "^7.0.1",
    "node-pty": "^0.7.8",
    "npmi": "^4.0.0",
    "performance-now": "^2.1.0",
    "rollup": "^1.1.2",
    "rollup-plugin-hypothetical": "^2.1.0",
    "rollup-plugin-vue": "^4.6.1",
    "serialport": "^7.1.1"
  },
  "devDependencies": {
    "@types/firmata": "^0.19.0",
    "@types/fs-extra": "^5.0.4",
    "@types/ip": "0.0.31",
    "@types/ncp": "^2.0.1",
    "@types/node": "^10.12.10",
    "@types/systemjs": "^0.20.6",
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "johnny-five": "^0.15.0",
    "mocha": "^5.2.0",
    "ncp": "^2.0.0",
    "spectron": "^3.8.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "keywords": [
    "Quark",
    "Electron"
  ],
  "author": "Nishkal Kashyap",
  "license": "ISC"
}
