{
  "name": "quark",
  "version": "0.1.0",
  "description": "A new way to control electronics",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "ts-node copyicons.ts && tsc && electron . ./quark.qrk",
    "link": "npm install ./../@squirtle/api",
    "native": "electron-builder install-app-deps",
    "compile": "tsc",
    "test": "mocha",
    "pack-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds",
    "dist-win": "electron-builder --x64 --win --prepackaged release-builds/Quark-win32-x64",
    "build-linux": "electron-builder --x64 --linux",
    "build-mac": "electron-builder --x64 --mac",
    "build-win": "npm run pack-win && npm run dist-win",
    "buil": "npm install && ts-node copyicons.ts && tsc && electron-builder .",
    "build": "npm install && ts-node copyicons.ts && tsc && ts-node scripts/build.ts"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@capacitor/electron": "^1.0.0-beta.0",
    "@squirtle/api": "file:../@squirtle/api",
    "@types/firmata": "^0.19.0",
    "@types/fs-extra": "^5.0.4",
    "@types/johnny-five": "0.0.40",
    "@types/node": "^10.12.10",
    "@types/serialport": "^7.0.1",
    "@vue/web-component-wrapper": "^1.2.0",
    "babel-loader": "^8.0.5",
    "builtin-modules": "^3.0.0",
    "chokidar": "^2.0.4",
    "css-loader": "^2.1.0",
    "electron-is-dev": "^0.3.0",
    "electron-store": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "firmata": "^2.0.0",
    "fs-extra": "^7.0.1",
    "johnny-five": "^0.15.0",
    "less-loader": "^4.1.0",
    "memory-fs": "^0.4.1",
    "node-pty": "^0.7.8",
    "node-sass": "^4.11.0",
    "npmi": "^4.0.0",
    "performance-now": "^2.1.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^1.0.0",
    "requireg": "^0.2.1",
    "rollup": "^1.1.2",
    "sass-loader": "^7.1.0",
    "serialport": "^7.1.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.1.6",
    "url-loader": "^1.1.2",
    "vscode-languageserver-types": "^3.14.0",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2",
    "electron-updater": "^4.0.6"
  },
  "devDependencies": {
    "@types/ip": "0.0.31",
    "@types/ncp": "^2.0.1",
    "chart.js": "^2.7.3",
    "devtron": "^1.4.0",
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "mocha": "^5.2.0",
    "ncp": "^2.0.0",
    "spectron": "^3.8.0",
    "ts-node": "^7.0.1"
  },
  "keywords": [
    "Quark",
    "Electron"
  ],
  "author": "Nishkal Kashyap",
  "license": "ISC"
}
