{
  "name": "quark",
  "author": "Nishkal Kashyap <hello@nishkal.in>",
  "email": "hello@nishkal.in",
  "homepage": "https://quarkjs.io",
  "version": "0.1.18",
  "description": "One software sketchbook for rapid prototyping and development of your projects.",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "land": "ts-node ./scripts/prepare-assets.ts && tsc && electron .",
    "start": "ts-node ./scripts/prepare-assets.ts && tsc && electron . ./quark.qrk",
    "qs": "electron . ./quark.qrk",
    "link": "npm install ./../@squirtle/api",
    "native": "electron-builder install-app-deps",
    "lic": "ts-node ./scripts/license.ts",
    "build-quick": "ts-node ./scripts/prepare-assets.ts && tsc && npm run lic && ts-node scripts/build.ts",
    "build-common": "npm install && npm run build-quick",
    "build-win": "npm run build-common && start .\\build",
    "build-linux": "ts-node ./scripts/build-linux.ts && cd ./../Quark-electron-linux && npm run build-common",
    "bw": "npm run build-win",
    "bl": "bash -c 'npm run build-linux'",
    "release": "ts-node ./scripts/release.ts",
    "upload": "ts-node ./scripts/upload.ts",
    "notes" : "ts-node ./scripts/create-release-notes.ts"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@squirtle/api": "file:./../@squirtle/api",
    "@vue/web-component-wrapper": "^1.2.0",
    "babel-loader": "^8.0.5",
    "builtin-modules": "^3.0.0",
    "chart.js": "^2.7.3",
    "chokidar": "^2.1.2",
    "css-loader": "^2.1.0",
    "electron-log": "^3.0.1",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "firmata": "^2.0.0",
    "fs-extra": "^7.0.1",
    "ionic": "^4.10.2",
    "johnny-five": "^0.15.0",
    "markdown-it": "^8.4.2",
    "memory-fs": "^0.4.1",
    "node-pty": "^0.7.8",
    "node-sass": "^4.11.0",
    "npm": "^6.8.0",
    "p5": "^0.7.3",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "serialport": "^7.1.4",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "url-loader": "^1.1.2",
    "vscode-languageserver-types": "^3.14.0",
    "vue": "^2.6.8",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^4.29.6",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@google-cloud/storage": "^2.4.2",
    "@types/chart.js": "^2.7.45",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/firmata": "^0.19.1",
    "@types/fs-extra": "^5.0.4",
    "@types/ip": "0.0.31",
    "@types/johnny-five": "0.0.40",
    "@types/license-checker": "^15.0.0",
    "@types/ncp": "^2.0.1",
    "@types/node": "^10.12.29",
    "@types/p5": "^0.7.2",
    "@types/progress-stream": "^2.0.0",
    "@types/recursive-readdir": "^2.2.0",
    "@types/serialport": "^7.0.1",
    "@types/sharp": "^0.21.2",
    "@types/single-line-log": "^1.1.0",
    "@types/table": "^4.0.5",
    "chalk": "^2.4.2",
    "devtron": "^1.4.0",
    "dotenv": "^6.2.0",
    "electron": "^4.1.3",
    "electron-builder": "^20.39.0",
    "electron-builder-squirrel-windows": "^20.40.0",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "license-checker": "^25.0.1",
    "mastodon-api": "^1.3.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "monaco-editor": "^0.15.6",
    "ncp": "^2.0.0",
    "progress-stream": "^2.0.0",
    "recursive-readdir": "^2.2.2",
    "rimraf": "^2.6.3",
    "sharp": "^0.22.0",
    "simple-git": "^1.110.0",
    "single-line-log": "^1.1.2",
    "spectron": "^3.8.0",
    "table": "^5.2.3",
    "ts-node": "^7.0.1"
  },
  "keywords": [
    "Quark",
    "Electron"
  ],
  "license": "ISC",
  "tilt": {
    "appId": "io.quarkjs",
    "copyright": "Copyright Â© 2019 Nishkal Kashyap",
    "productName": "Quark",
    "artifactName": "${productName}-${os}-${arch}-${version}.${ext}",
    "asar": true,
    "asarUnpack": [
      "definitions-unpacked"
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!dist",
      "!build",
      "!.vscode",
      "!definitions"
    ],
    "directories": {
      "output": "build",
      "buildResources": "buildResources"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE",
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "linux": {
      "target": "AppImage"
    },
    "appImage": {
      "systemIntegration": "ask",
      "license": "LICENSE"
    },
    "mac": {
      "target": "default",
      "category": "public.app-category.utilities"
    },
    "extraResources": [
      {
        "from": "definitions",
        "to": "definitions"
      }
    ]
  }
}
