{
  "name": "quark",
  "version": "0.1.5",
  "description": "A new way to control electronics",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "ts-node ./scripts/copyicons.ts && tsc && electron . ./quark.qrk",
    "land": "ts-node ./scripts/copyicons.ts && tsc && electron .",
    "link": "npm install ./../@squirtle/api",
    "compile": "tsc",
    "native": "electron-builder install-app-deps",
    "lic": "ts-node ./scripts/license.ts",
    "test": "mocha",
    "pack-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds",
    "dist-win": "electron-builder --x64 --win --prepackaged release-builds/Quark-win32-x64",
    "build-linux": "electron-builder --x64 --linux",
    "build-mac": "electron-builder --x64 --mac",
    "build-win": "npm run pack-win && npm run dist-win",
    "buil": "npm install && ts-node ./scripts/copyicons.ts && tsc && electron-builder .",
    "build": "npm install && ts-node ./scripts/copyicons.ts && tsc && npm run lic && ts-node scripts/build.ts"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@squirtle/api": "file:../@squirtle/api",
    "@vue/web-component-wrapper": "^1.2.0",
    "angular": "^1.7.7",
    "babel-loader": "^8.0.5",
    "builtin-modules": "^3.0.0",
    "chart.js": "^2.7.3",
    "chokidar": "^2.1.1",
    "css-loader": "^2.1.0",
    "electron-log": "^3.0.1",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "firmata": "^2.0.0",
    "fs-extra": "^7.0.1",
    "ionic": "^4.10.2",
    "johnny-five": "^0.15.0",
    "markdown-it": "^8.4.2",
    "memory-fs": "^0.4.1",
    "node-pty": "^0.7.8",
    "node-sass": "^4.11.0",
    "npm": "^6.8.0",
    "pacote": "^9.5.0",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "serialport": "^7.1.4",
    "ts-loader": "^5.3.3",
    "typescript": "^3.1.6",
    "url-loader": "^1.1.2",
    "vscode-languageserver-types": "^3.14.0",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@google-cloud/storage": "^2.4.2",
    "@types/chart.js": "^2.7.45",
    "@types/dotenv": "^6.1.0",
    "@types/firmata": "^0.19.1",
    "@types/fs-extra": "^5.0.4",
    "@types/ip": "0.0.31",
    "@types/johnny-five": "0.0.40",
    "@types/license-checker": "^15.0.0",
    "@types/ncp": "^2.0.1",
    "@types/node": "^10.12.10",
    "@types/recursive-readdir": "^2.2.0",
    "@types/serialport": "^7.0.1",
    "@types/sharp": "^0.21.2",
    "devtron": "^1.4.0",
    "dotenv": "^6.2.0",
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5",
    "glob": "^7.1.3",
    "license-checker": "^25.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "monaco-editor": "^0.15.6",
    "ncp": "^2.0.0",
    "recursive-readdir": "^2.2.2",
    "sharp": "^0.21.3",
    "spectron": "^3.8.0",
    "ts-node": "^7.0.1"
  },
  "keywords": [
    "Quark",
    "Electron"
  ],
  "author": "Nishkal Kashyap",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/ansi-bgyellow.git"
  },
  "tilt": {
    "appId": "io.quarkjs",
    "copyright": "Copyright Â© 2019 Nishkal Kashyap",
    "productName": "Quark",
    "artifactName": "${productName}-${os}-${arch}-${version}.${ext}",
    "asar": true,
    "asarUnpack": [
      "definitions-unpacked"
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!dist",
      "!build",
      "!.vscode",
      "!definitions"
    ],
    "directories": {
      "output": "build",
      "buildResources": "buildResources"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE",
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "linux": {
      "target": "AppImage"
    },
    "appImage": {
      "systemIntegration": "ask",
      "license": "LICENSE"
    },
    "mac": {
      "target": "default",
      "category": "public.app-category.utilities"
    },
    "extraResources": [
      {
        "from": "definitions",
        "to": "definitions"
      }
    ]
  }
}
